@using Saned.Jazan.ControlPanel.Extensions
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge;charset=utf-8" />
    <title>جازان</title>
    <link href="/Content/dist/img/ico.ico" rel="icon" />
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
 
    <link href="~/Content/dist/css/prettyPhoto.css" rel="stylesheet"/>
  
    
    <script type="text/javascript">
        // When the document is ready
        $(document).ready(function () {
            //$('#example1').datepicker({
            //    format: "dd/mm/yyyy"
            //});
        });


      

    

    </script>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/kendo/css")
    @Scripts.Render("~/bundles/modernizr")


    <style>
        .dataTables_empty {
            color: #DD4B39;
        }
   
        .connectedSortable {
            min-height: 0px;
        }

        .nav-tabs-custom {
            display: none;
        }

        .fc-view-container,
        .fc-toolbar {
            display: none;
        }
    </style>
    <meta name="description" content="The description of my page" />
</head>

<body class="hold-transition skin-blue sidebar-mini">
    <!-- Loader -->
    <div id="ajaxLoaderDiv" class="ripple" style="display: none;">
        <img src="/Content/dist/img/ripple.gif" class="ajax-loader" />
    </div>
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels --><span class="logo-mini">
                    <img src="/Content/dist/img/ico.png" style="width: 65%;" />
                </span>
                <!-- logo for regular state and mobile devices --><span class="logo-lg"><b>جازان</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button"> <span class="sr-only">Toggle navigation</span> </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"> <img src="/Content/dist/img/ico.png" class="user-image" alt="User Image"> <span class="hidden-xs">مدير الموقع</span> </a>

                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="/Content/dist/img/ico.png" class="img-circle" alt="User Image" />
                                    <p>
                                        مدير الموقع
                                        @*<small>عضو منذ نوفمبر 2016</small>*@
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="@Url.Action("ChangePassword", "Account")" class="btn btn-default btn-flat">تغير كلمة السر</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="@Url.Action("Logout", "Account")" class="btn btn-default btn-flat">تسجيل الخروج</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                     
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="treeview @Html.IsSelected("Home", "Index")">
                        <a href="@Url.Action("Index", "Home")"> <i class="fa fa-dashboard"></i> <span>الصفحة الرئيسية</span> </a>
                    </li>

                    @*<li class="header">الصفحات</li>*@
                    <li class="treeview @Html.IsSelected("Categories", "Index")">
                        <a onclick="showPageLoadingSpinner()" href="@Url.Action("Index", "Categories")"> <i class="glyphicon glyphicon-tags"></i> <span>التصنيفات</span> </a>
                    </li>
                    <li class="treeview @Html.IsSelected("Index", "ViewNews")">
                        <a onclick="showPageLoadingSpinner()" href="@Url.Action("Index", "News")"> <i class="fa fa-newspaper-o"></i> <span>الاخبار</span> </a>
                    </li>

                    <li class="treeview @Html.IsSelected("Advertisement", "Index")">
                        <a onclick="showPageLoadingSpinner()" href="@Url.Action("Index","Advertisement")"> <i class="fa fa-buysellads"></i> <span>الأعلانات</span> </a>
                    </li>

                    <li class="treeview @Html.IsSelected("Package", "Index")">
                        <a onclick="showPageLoadingSpinner()" href="@Url.Action("Index","Package")">
                            <i class="fa fa-gift"></i> <span>الباقات</span>
                        </a>
                    </li>


                    <li class="treeview @Html.IsSelected("Feature", "Index")">
                        <a onclick="showPageLoadingSpinner()" href="@Url.Action("Index","Feature")"> <i class="glyphicon glyphicon-briefcase"></i> <span>الميزات</span> </a>
                    </li>

                    <li class="treeview @Html.IsSelected("Users", "Index")">
                        <a onclick="showPageLoadingSpinner()" href="@Url.Action("Index", "Users")"> <i class="fa fa-user"></i> <span> المستخدمين </span> </a>
                    </li>
                    <li class="treeview @Html.IsSelected("TouristVisit", "Index")">
                        <a onclick="showPageLoadingSpinner()" href="@Url.Action("Index", "TouristVisit")"><i class="fa fa-tripadvisor"></i> <span>الزيارات السياحية</span> </a>
                    </li>
                    <li class="treeview @Html.IsSelected("Competition", "Index")">
                        <a onclick="showPageLoadingSpinner()" href="@Url.Action("Index", "Competition")"> <i class="fa fa-trophy"></i> <span> المسابقة الثقافية</span> </a>
                    </li>

                    <li class="treeview @Html.IsSelected("AboutUs", "AboutUs")">
                        <a onclick="showPageLoadingSpinner()" href="@Url.Action("AboutUs", "AboutUs")"> <i class="fa fa-phone"></i> <span> معلومات الاتصال</span> </a>
                    </li>

                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        
        @* Render Body here ------  *@
          @Html.Partial("_FlashMessage")
        @RenderBody()
        <footer class="main-footer"> <strong>جميع الحقوق محفوظة &copy; 2015-2016 <a href="#">جازان</a>.</strong> </footer>
    </div>
    <!-- ./wrapper -->
    <!-- Modal -->
    <div class="modal fade" id="Delete" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel1">حذف العنصر</h4>
                </div>
                <form>
                    <div class="modal-body"> هل أنت متأكد من أنك تريد الحذف ؟ </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-success js-delete-yes">نعم</button>
                        <button type="button" class="btn btn-danger js-delete-no" data-dismiss="modal">لا</button>
                    </div>
                </form>
            </div>
        </div>
    </div>



    @Scripts.Render("~/bundles/jquery")
    @*<script src="https://code.jquery.com/jquery-3.0.0.js"></script>
    <script src="https://code.jquery.com/jquery-migrate-3.0.0.js"></script>*@
@*<script src="http://code.jquery.com/jquery-1.7.2.min.js"></script>*@
@Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/kendo")
    @Scripts.Render("~/bundles/js")

    
    @RenderSection("scripts", required: false)
    <script src="@Url.Content("~/Scripts/jquery.cookie.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.flashmessage.js")"></script>
    @*<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap3-dialog/1.35.4/css/bootstrap-dialog.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap3-dialog/1.35.4/js/bootstrap-dialog.min.js"></script>*@

    <link rel="stylesheet" href="~/Content/dist/css/bootstrap-dialog.min.css">
    <script src="~/Content/dist/js/bootstrap-dialog.min.js"></script>
@Scripts.Render("~/bundles/unobtrusive")
  

</body>

</html>
<script src="~/Scripts/jquery.prettyPhoto.js"></script>
<script type="text/javascript">
    function initPrettyPhoto() {
        $("a[rel='prettyPhoto[pp_gal]']").prettyPhoto({
            show_title: false,
            social_tools: false,
            deeplinking: false,
        });
        $("a[rel='prettyPhoto']").prettyPhoto({

            show_title: false,
            social_tools: false,
            deeplinking: false,

        });
    }
    function pagingSuccess() {
        initPrettyPhoto();
    }
    $(document).ready(function () {

        initPrettyPhoto();

        $('[data-toggle="tooltip"]').tooltip();


        $("#clear").click(function () {
            $(".k-datepicker input").val('');
        });
       



        $(document).on('submit',
            'form',
            function () {
                var buttons = $(this).find('[type="submit"]');

                if ($(this).attr("data-ajax") == undefined)
                {
                    buttons.each(function (btn) {
                    $(buttons[btn]).prop('disabled', true);
                  });
                }

            });
    });


    function showPageLoadingSpinner() {
        //$('.ajax-loader').remove();
        //$('#ajaxLoaderDiv').hide();
        $('#ajaxLoaderDiv').show();
        //$('#ajaxLoaderDiv').append(
        //    ' <img src="/Content/dist/img/ripple.gif"' + ' class="ajax-loader"' + '/>'
        //);
    }

    function hidePageLoadingSpinner() {
        setTimeout(function () {
            //$('.ajax-loader').remove();
            $('#ajaxLoaderDiv').hide();
          


        }, 20000);
    }

</script>
<style>
    .pp_previous, .pp_next, .pp_expand {
        display: none !important;
    }
</style>
